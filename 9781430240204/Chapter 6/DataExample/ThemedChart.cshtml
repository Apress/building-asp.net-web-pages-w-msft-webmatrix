@{
    var sales = new Dictionary<string, int>()
        {
            { "Books", 10 },
            { "DVDs", 7 },
            { "Games", 12 },
            { "Music", 5 },
            { "Toys", 9 }
        };
    
    var myChart = new Chart(width: 600, height: 400, theme: ChartTheme.Green)
        .AddTitle("Sales")
        .AddLegend("Legend:", "SalesLegend")
        .AddSeries(
            name: "Sales Per Product",
            xValue: sales.Keys,
            yValues: sales.Values)
        .Write();
}

